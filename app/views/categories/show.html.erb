<script>
document.addEventListener('DOMContentLoaded', function() {
  const filterForm = document.querySelector('.filters-form');
  
  if (filterForm) {
    // Auto-submit when radio buttons change
    const radioInputs = filterForm.querySelectorAll('input[type="radio"]');
    radioInputs.forEach(radio => {
      radio.addEventListener('change', function() {
        // Update visual state
        const container = this.closest('.radio-options');
        if (container) {
          container.querySelectorAll('.radio-option').forEach(opt => {
            opt.classList.remove('active');
          });
          this.closest('label').classList.add('active');
        }
        // Submit form
        setTimeout(() => filterForm.submit(), 150);
      });
    });
    
    // Auto-submit when sort dropdown changes
    const sortSelect = filterForm.querySelector('.filter-select');
    if (sortSelect) {
      sortSelect.addEventListener('change', function() {
        filterForm.submit();
      });
    }
    
    // Submit search on Enter
    const searchInput = filterForm.querySelector('.filter-input');
    if (searchInput) {
      searchInput.addEventListener('keypress', function(e) {
        if (e.key === 'Enter') {
          e.preventDefault();
          filterForm.submit();
        }
      });
    }
  }
});
</script>
